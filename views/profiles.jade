extends layout

block content
  #main
    .sub
      .title
        |typographer
      .featurette
    include partials/nav
    #newForm
      #headerBox
        #header #{page}
      p #{data.username}'s artwork collection:
      .userArtwork
        if data.artworkId
          each item in data.artworkId
            .item 
              a.artwork(href="/view/"+data._id+"/"+item.id)= item.name
            .edit
              a.artwork(href="/edit/"+data._id+"/"+item.id) edit
            .delete
              a.artwork(href="/delete/"+data._id+"/"+item.id) x
      
