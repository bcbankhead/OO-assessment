extends layout

block content
  #main
    .sub
      .title
        |typographer
      .featurette
    include partials/nav
    #newForm
      p #{data.username}'s artwork collection:
      .userArtwork
        if data.artworkId
          each item in data.artworkId
            .item 
              a.artwork(href="/view/"+data._id+"/"+item.id)= item.name
    
      
